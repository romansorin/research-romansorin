{
  "compilerOptions": {
    "target": "es2017",
    "allowSyntheticDefaultImports": false,
    "baseUrl": "./",
    "paths": {
      "Components/*": ["src/components/*"],
      "Firebase/*": ["firebase/*"],
      "Models/*": ["src/models/*"],
      "Icons/*": ["src/icons/*"],
      "Images/*": ["src/images/*"],
      "Pages/*": ["src/pages/*"]
    }
  },
  "exclude": ["node_modules", "dist", "public", ".cache"]
}